{
	"appId": "${ANDROID_APPLICATION_ID}",
	"appName": "alaatv",
	"bundledWebRuntime": false,
	"npmClient": "yarn",
	"webDir": "www",
	"plugins": {
		"DeepLinks": {
			"schemes": [
				"https"
			],
			"hosts": [
				"alaatv.com"
			],
			"androidPathPrefix": "/",
			"customUrlScheme": "alaatvapp"
		}
	},
	"server": {
		"androidScheme": "https"
	}
}
