{
  "appId": "${ANDROID_APPLICATION_ID}",
  "appName": "${ANDROID_APP_NAME}",
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "webDir": "www",
  "plugins": {
    "DeepLinks": {
      "schemes": [
        "https"
      ],
      "hosts": [
        "alaatv.com"
      ],
      "androidPathPrefix": "/",
      "customUrlScheme": "alaatvapp"
    }
  },
  "server": {
    "iosScheme": "https",
    "hostname": [
      "alaatv.com"
    ],
    "androidScheme": "https",
    "url": "http://192.168.3.102:8083",
    "allowNavigation": [
      "alaatv.com",
      "*.alaatv.com",
      "*"
    ]
  }
}
