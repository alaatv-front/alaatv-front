{
  "appId": "${ANDROID_APPLICATION_ID}",
  "appName": "${ANDROID_APP_NAME}",
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "webDir": "www",
  "plugins": {
    "DeepLinks": {
      "schemes": [
        "https"
      ],
      "hosts": [
        "alaatv.com"
      ],
      "androidPathPrefix": "/",
      "customUrlScheme": "alaatvapp"
    }
  },
  "server": {
    "androidScheme": "https"
  }
}