{
  "name": "alaatv",
  "version": "1.5.0",
  "description": "alaatv with vite",
  "productName": "alaatv",
  "author": "Ali <aliesmaeeli20@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --fix --ext .js,.vue ./",
    "lint:style": "stylelint --custom-syntax postcss-html --config stylelint-config-recommended-vue.json --fix '**/*.{vue,scss}'\n",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "ssr": "quasar dev -m ssr",
    "prebuild": "node inject-env-vars.js",
    "build": "quasar build",
    "ci": "rm -rf node_modules && yarn install --production --frozen-lockfile",
    "dev:ssr": "quasar dev -m ssr",
    "dev:android": "quasar dev -m capacitor -T android",
    "build:android": "quasar build -m capacitor -T android",
    "build:pwa": "quasar build -m pwa",
    "build:ssr": "quasar build --mode ssr",
    "pm2start": "pm2 start dist/ssr/index.js",
    "start:ssr": "yarn ci && yarn build:ssr && cd dist/ssr && yarn install && yarn start",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
